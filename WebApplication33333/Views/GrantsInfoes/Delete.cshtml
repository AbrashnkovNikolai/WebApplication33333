@model WebApplication33333.GrantsInfo

@{
    ViewData["Title"] = "Delete";
}

<h1>удалить</h1>

<h3>Вы уверены что хотите удалить эту запись?</h3>
<div>
    <h4>GrantsInfo</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            Название стипендии
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.GrantName)
        </dd>
        <dt class = "col-sm-2">
			Размер выплаты бакалаврам
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.BanclorGrantValue)
        </dd>
        <dt class = "col-sm-2">
			Размер выплаты магистрам
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.MasterGrantValue)
        </dd>
        <dt class = "col-sm-2">
            Размер выплаты аспирантам
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.AspirantGrantValue)
        </dd>
        <dt class = "col-sm-2">
            Idстипендии
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.GrantNameId)
        </dd>
    </dl>
    
    <form asp-action="Delete">
        <input type="hidden" asp-for="GrantName" />
        <input type="submit" value="Удалить" class="btn btn-danger" /> |
        <a asp-action="Index">Вернуться к таблице</a>
    </form>
</div>
